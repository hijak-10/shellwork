{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "customer",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "filter1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          C_CUSTKEY as integer,",
				"          C_NAME as string,",
				"          C_ADDRESS as string,",
				"          C_NATIONKEY as integer,",
				"          C_PHONE as string,",
				"          C_ACCTBAL as float,",
				"          C_MKTSEGMENT as string,",
				"          C_COMMENT as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source1 filter(C_NAME!=null && C_ACCTBAL!=null && C_ADDRESS!=null && C_MKTSEGMENT!=null && C_NATIONKEY!=null && C_PHONE!=null) ~> filter1"
			]
		}
	}
}